{
    "manifest_version": 2,
    "name": "Password Manager",
    "description": "This extension allows the user to manage secure password for online applications.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}